<div data-role="content">
    <div class="content-primary">
        <form action="" method="post">
            <div data-role="fieldcontain">
                <label for="cardholders_name">Cardholder's Name:</label>
                <input class="required" type="text" id="cardholders_name" name="cardholders_name" maxlength="100">

            </div>
            <div data-role="fieldcontain">
                <label for="card_number">Credit Card Number:</label>
                <input class="required" type="text" id="card_number" name="card_number" maxlength="100">
            </div>
            <div data-role="fieldcontain">
                <label for="card_number">Expiration Date:</label>
                <select name="select-choice-0" id="select-choice-0">
                    <option value="">Month</option>
                    <option value="1">1</option>
                    <option value="2">2</option>
                    <option value="3">3</option>
                    <option value="4">4</option>
                    <option value="5">5</option>
                    <option value="6">6</option>
                    <option value="7">7</option>
                    <option value="8">8</option>
                    <option value="9">9</option>
                    <option value="10">10</option>
                    <option value="11">11</option>
                    <option value="12">12</option>
                </select>
            </div>
            <div data-role="fieldcontain">
                <label for="payment_card_type">Credit Card Type:</label>
                <input type="text" style="display:none" id="payment_card_type">
                <div style="width:78%;display:inline-block">
                    <div id="payment_card_type" style="display:inline-block;height:26px;width:172px;background-image:url(/img/card_types.png)">
                        &nbsp;
                    </div>
                </div>
            </div>
            <input type="submit" value="Register">
        </form>
    </div>
</div>
